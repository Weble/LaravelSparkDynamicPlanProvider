{
    "name": "webleit/laravel-spark-dynamic-plan-provider",
    "version": "1.0.0",
    "homepage": "https://github.com/webleit/LaravelSparkDynamicPlanProvider",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniele Rosario",
            "email": "daniele@weble.it",
            "homepage": "https://www.weble.it",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~7.0",
        "illuminate/support": "~5.5.0|~5.6.0",
        "illuminate/database": "~5.5.0|~5.6.0",
        "illuminate/cache": "~5.5.0|~5.6.0",
        "stripe/stripe-php": "~5.0",
        "laravel/cashier": "~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~7.0"
    },
    "autoload": {
        "psr-4": {
            "Webleit\\LaravelSparkDynamicPlanProvider\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webleit\\LaravelSparkDynamicPlanProvider\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webleit\\LaravelSparkDynamicPlanProvider\\DynamicPlanServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
